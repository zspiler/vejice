{
    "manifest_version": 2,
    "name": "Vejice",
    "version": "1.0",
  
    "icons": {
      "48": "icon.png"
    },

    "permissions": [
        "activeTab",
        "http://178.128.193.133:5050/*"
    ],

    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Vejice",
        "default_popup": "popup.html"
    },
  
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["script.js"]
      }
    ]
  
  }
  